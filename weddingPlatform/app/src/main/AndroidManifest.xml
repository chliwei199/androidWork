<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ron.idv.drawerlayoutdemo">

    <!--
    To use Toolbar as an Action Bar, first disable the decor-provided Action Bar.
    The easiest way is to have your theme extend from Theme.AppCompat.NoActionBar
    (or its light variant).
    -->
    <application
        android:allowBackup="true"
        android:icon="@mipmap/wedding_logo"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
